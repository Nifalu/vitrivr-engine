    "parameters": {
      "path": "/Volumes/V3C1/V3C1/videos",
      "depth": "1",
      "mediaTypes": "VIDEO"
    },
    "next": {
      "name": "VideoDecoder",
      "nextTransformer": {
        "name": "PassthroughTransformer",
        "nextSegmenter": {
          "name": "FixedDurationSegmenter",
          "parameters": {
            "duration": "1",
            "lookAheadTime": "1"
          },
          "aggregators": [
            {
              "name": "FirstContentAggregator",
              "nextExtractor": {
                "fieldName": "averagecolor1",
                "nextExporter": {
                  "exporterName": "thumbnail"
                }
              }
            }
          ]
        }
      }
    }
  }
}