{
  "connection": {
    "database": "PgVectorConnectionProvider",
    "parameters": {
      "host": "127.0.0.1",
      "port": "5432",
      "username": "postgres",
      "password": "admin"
    }
  },
  "fields": {
    "averagecolor": { "factory": "AverageColor" },
    "file": {
      "factory": "FileSourceMetadata",
      "indexes": [
        {
          "attributes": [
            "path"
          ],
          "type": "FULLTEXT"
        },
        {
          "attributes": [
            "size"
          ],
          "type": "SCALAR"
        }
      ]
    },
    "time": { "factory": "TemporalMetadata" },
    "video": { "factory": "VideoSourceMetadata" }
  }
}