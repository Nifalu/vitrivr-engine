{
  "name": "vitrivr-test",
  "connection": {
    "database": "CottontailConnectionProvider",
    "parameters": {
      "host": "127.0.0.1",
      "port": "1865"
    }
  },
  "fields": [
    {
      "name": "averagecolor",
      "factory": "AverageColor"
    },
    {
      "name": "file",
      "factory": "FileSourceMetadata",
      "indexes": [
        {
          "attributes": [
            "path"
          ],
          "type": "FULLTEXT"
        },
        {
          "attributes": [
            "size"
          ],
          "type": "SCALAR"
        }
      ]
    },
    {
      "name": "time",
      "factory": "TemporalMetadata"
    },
    {
      "name": "video",
      "factory": "VideoSourceMetadata"
    }
  ],
  "exporters": []
}
