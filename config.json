{
  "schemas": [
    {
      "name": "vitrivr",
      "connection": {
        "database": "CottontailConnectionProvider",
        "parameters": {
          "host": "127.0.0.1",
          "port": "1865"
        }
      },
      "fields": [
        {
          "name": "averagecolor",
          "factory": "AverageColor",
          "parameters": {
            "key": "AverageColor-value-schema"
          }
        },
        {
          "name": "file",
          "factory": "FileMetadata",
          "parameters": {
            "key": "FileMetadata-value-schema"
          }
        },
        {
          "name": "time",
          "factory": "TemporalMetadata",
          "parameters": {
            "key": "TemporalMetadata-value-schema"
          }
        },
        {
          "name": "clip",
          "factory": "CLIPImage"
        },
        {
          "name": "dino",
          "factory": "DINO"
        }
      ],
      "exporters": [
        {
          "name": "thumbnail",
          "factory": "ThumbnailExporter",
          "parameters": {
            "key": "ThumbnailExporter-value-schema"
          },
          "resolver": {
            "factory": "DiskResolver",
            "parameters": {
              "key": "DiskResolver-value-schema"
            }
          }
        }
      ],
      "extractionPipelines" : [
        {
          "name": "image",
          "path": "./config-pipelines/image-pipeline.json"
        },
        {
          "name": "video",
          "path": "./config-pipelines/video-pipeline.json"
        }
      ]
    },
    {
      "name": "VBSLHE",
      "connection": {
        "database": "CottontailConnectionProvider",
        "parameters": {
          "host": "127.0.0.1",
          "port": "1865"
        }
      },
      "fields": [
        {
          "name": "averagecolor",
          "factory": "AverageColor"
        },
        {
          "name": "clip",
          "factory": "CLIPImage"
        },
        {
          "name": "dino",
          "factory": "DINO"
        },
        {
          "name": "file",
          "factory": "FileMetadata"
        },
        {
          "name": "time",
          "factory": "TemporalMetadata"
        }
      ],
      "exporters": [
        {
          "name": "thumbnail",
          "factory": "ThumbnailExporter",
          "parameters": {
            "key": "ThumbnailExporter-value-schema"
          },
          "resolver": {
            "factory": "DiskResolver",
            "parameters": {
              "key": "DiskResolver-value-schema"
            }
          }
        }
      ]
    }
  ]
