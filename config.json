              }
            },
          {
              "name": "asr",
              "factory": "ASR",
                "parameters": {
                  "host": "http://10.34.58.14:5003/",
                  "model": "whisper"
                }
          },
          {
            "name": "ocr",
            "factory": "OCR",
            "parameters": {
              "host": "http://10.34.58.14:5004/",
              "model": "tesseract"
            }
          },
          {
            "name": "time",
            "factory": "TemporalMetadata"
          }
        ]
    }
  ]
}