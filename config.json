{
  "schemas": [
    {
      "name": "vitrivr",
      "connection": {
        "database": "CottontailConnectionProvider",
        "parameters": {
          "host": "127.0.0.1",
          "port": "1865"
        }
      },
      "fields": [
        {"name": "averagecolor", "factory": "AverageColor"},
        {"name": "file", "factory": "FileMetadata"},
        {"name": "time", "factory": "TemporalMetadata"}
      ],
      "exporters": [
        {
          "name": "thumbnail",
          "factory": "ThumbnailExporter",
          "parameters": {},
          "resolver": {
            "factory": "DiskResolver",
            "parameters": {}
          }
        }
      ]
    }
  ]
}